{
  "name": "lazyscripts.js",
  "title": "LazyScripts",
  "version": "0.4.0",
  "description": "Delay script loading until user interaction.",
  "keywords": [
    "lazy",
    "delay",
    "loading"
  ],
  "license": "MIT",
  "author": {
    "name": "Gene Alyson Fortunado Torcende",
    "email": "genealyson.torcende@gmail.com"
  },
  "files": [
    "/dist"
  ],
  "browser": "dist/cdn.min.js",
  "unpkg": "dist/cdn.min.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kermage/lazyscripts.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/module.d.mts",
        "default": "./dist/module.mjs"
      },
      "require": {
        "types": "./dist/module.d.cts",
        "default": "./dist/module.cjs"
      }
    }
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.11",
    "jsdom": "^26.0.0",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0",
    "vitest": "^3.0.9"
  },
  "scripts": {
    "test": "vitest",
    "build": "unbuild",
    "prepublishOnly": "npm run build"
  }
}
